{
    "name": "klink/pronto",
    "description": "The Pronto CMS base Framework.",
    "keywords": ["framework", "pronto", "cms", "php"],
    "license": "GPL-3.0",
    "version": "0.2.0",
    "authors": [
        {
            "name": "Alessio Vertemati",
            "email": "avsoft@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/lumen-framework": "~5.3.0",
        "vlucas/phpdotenv": "~2.2",
        "intervention/image": "~2.3",
        "erusev/parsedown": "~1.6.0",
        "erusev/parsedown-extra": "~0.7.1",
        "symfony/yaml": "~3.0.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "Pronto\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]

{
    "name": "avvertix/pronto-framework",
    "description": "The Pronto CMS base Framework.",
    "keywords": ["framework", "pronto", "cms", "php"],
    "license": "GPL-3.0",
    "homepage": "https://github.com/avvertix/pronto-framework",
    "support": {
        "issues": "https://github.com/avvertix/pronto-framework/issues",
        "source": "https://github.com/avvertix/pronto-framework"
    },
    "authors": [
        {
            "name": "Alessio Vertemati",
            "email": "avsoft@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "@dev",
        "vlucas/phpdotenv": "~1.0",
        "intervention/image": "~2.3",
        "erusev/parsedown": "~1.6",
        "erusev/parsedown-extra": "0.7.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "Pronto\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "minimum-stability": "dev"
}
